{
  "name": "coa-data-processing",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/cityofasheville/coa-data-processing.git",
  "author": "Eric Jackson <ejackson@ashevillenc.gov>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "no-console": 0,
      "max-len": [
        "error",
        100,
        {
          "ignoreTrailingComments": true
        }
      ]
    },
    "env": {
      "mocha": true
    }
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "snyk": "^1.316.1"
  },
  "scripts": {
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "snyk": true
}
